                      <li><a href="{{ path('login') }}">Se connecter</a></li>
                    {% else %}
                      <li><a href="{{ path('profile') }}">Profil</a></li>
                      <li><a href="{{ path('logout') }}">Se déconnecter</a></li>
                    {% endif %}

                    {% if is_granted('ROLE_ADMIN') %}
                    <li><a href="{{ path('admin_home') }}" class="btn btn-success"> Aller sur le back-office </a></li>
                    {% endif %}
                    </ul>
                  </div>
                </nav>
              </div>
            </div>
          </div>
        </div>
